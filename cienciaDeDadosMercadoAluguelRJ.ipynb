# Commented out IPython magic to ensure Python compatibility.
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns
from scipy import stats
from sklearn.linear_model import LinearRegression
import random

# Configura√ß√µes de visualiza√ß√£o
# %matplotlib inline
sns.set(style="whitegrid")
plt.rcParams['figure.figsize'] = (10, 6)

np.random.seed(42)
random.seed(42)

# Criando dados mockados
num_imoveis = 500
bairros = ['Copacabana', 'Ipanema', 'Leblon', 'Botafogo', 'Flamengo', 'Laranjeiras', 'Santa Teresa']
tipos_imovel = ['Apartamento', 'Casa', 'Quarto', 'Studio']

data = {
    'id': range(1, num_imoveis+1),
    'bairro': np.random.choice(bairros, num_imoveis),
    'tipo_imovel': np.random.choice(tipos_imovel, num_imoveis, p=[0.6, 0.2, 0.15, 0.05]),
    'quartos': np.random.randint(1, 5, num_imoveis),
    'banheiros': np.random.randint(1, 4, num_imoveis),
    'avaliacoes': np.random.poisson(15, num_imoveis),
    'dias_disponiveis': np.random.randint(30, 366, num_imoveis)
}

df = pd.DataFrame(data)
base_price = {
    'Copacabana': 300, 'Ipanema': 350, 'Leblon': 400,
    'Botafogo': 250, 'Flamengo': 220, 'Laranjeiras': 200, 'Santa Teresa': 180
}

df['preco_base'] = df['bairro'].map(base_price)
df['preco'] = (df['preco_base'] *
               (1 + 0.1 * (df['quartos'] - 1)) *  # Cada quarto extra aumenta 10%
               (1 + 0.05 * (df['banheiros'] - 1)) *  # Cada banheiro extra aumenta 5%
               (1 + np.random.normal(0, 0.1, num_imoveis)))  # Varia√ß√£o aleat√≥ria


df['preco'] = df['preco'].round(2)
df = df.drop(columns=['preco_base'])


outlier_indices = np.random.choice(num_imoveis, size=int(num_imoveis*0.05), replace=False)
df.loc[outlier_indices, 'preco'] *= 3

# Visualizando os primeiros registros
df.head()

"""##Medidas de Tend√™ncia Central"""

# C√°lculo das medidas
media = df['preco'].mean()
mediana = df['preco'].median()
moda = df['preco'].mode()[0]

print(f"Medidas de Tend√™ncia Central:")
print(f"M√©dia: R${media:.2f}")
print(f"Mediana: R${mediana:.2f}")
print(f"Moda: R${moda:.2f}")

# Visualiza√ß√£o
plt.figure(figsize=(10, 5))
sns.histplot(df['preco'], bins=30, kde=True)
plt.axvline(media, color='r', linestyle='--', label=f'M√©dia (R${media:.2f})')
plt.axvline(mediana, color='g', linestyle='-', label=f'Mediana (R${mediana:.2f})')
plt.title("Distribui√ß√£o de Pre√ßos com Medidas de Tend√™ncia Central")
plt.legend()
plt.show()

"""##Medidas de Dispers√£o"""

amplitude = df['preco'].max() - df['preco'].min()
variancia = df['preco'].var()
desvio_padrao = df['preco'].std()

print(f"üìà Medidas de Dispers√£o:")
print(f"Amplitude: R${amplitude:.2f}")
print(f"Vari√¢ncia: {variancia:.2f}")
print(f"Desvio Padr√£o: R${desvio_padrao:.2f}")

plt.figure(figsize=(10, 4))
sns.boxplot(x=df['preco'])
plt.title("Boxplot - Dispers√£o dos Pre√ßos")
plt.show()

"""##Visualiza√ß√µes Estat√≠sticas"""

